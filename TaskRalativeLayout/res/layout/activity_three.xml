                    android:id="@+id/buttonp2OnActivity3_2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/buttonp1OnActivity3_2"
                    android:text="p2" />
            </RelativeLayout>
        </ScrollView>

        <HorizontalScrollView
            android:id="@+id/scrollHorizontalanother"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:layout_margin="5dp"
            android:layout_toLeftOf="@+id/scrollVerticle" >

