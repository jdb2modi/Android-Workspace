                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@+id/buttonNext1OnActivity3another"
                    android:text="p1" />

                <Button
                    android:id="@+id/buttonp2OnActivity3another"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@+id/buttonp1OnActivity3another"
