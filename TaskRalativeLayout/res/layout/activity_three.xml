                    android:layout_toRightOf="@+id/buttonp1OnActivity31"
                    android:text="p2" />

                <Button
                    android:id="@+id/buttonp3OnActivity31"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
