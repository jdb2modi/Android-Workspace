                    android:id="@+id/buttonp3OnActivity3another"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@+id/buttonp2OnActivity3another"
                    android:text="p3" />

                <Button
                    android:id="@+id/buttonp4OnActivity3another"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@+id/buttonp3OnActivity3another"
                    android:text="p4" />
            </RelativeLayout>
        </HorizontalScrollView>

        <HorizontalScrollView
            android:id="@+id/scrollHorizontalanothe"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:layout_margin="5dp"
